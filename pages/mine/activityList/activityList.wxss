/* pages/mine/activityList/activityList.wxss */
.list{
    background: #FFF;
    margin: 26rpx 24rpx;
    border-radius: 20rpx;
    padding: 0 26rpx;
}
.item{

    height: 231rpx;
}
.item:not(:last-child){
    border-bottom: 1rpx solid #d5d5d5;
}
.head{
    height: 164rpx;
    width: 220rpx;
    border-radius: 20rpx;
    margin-top: 30rpx;
    margin-right: 42rpx;
    vertical-align: -130rpx;
}
.right{
    display: inline-block;
    width: 370rpx;
    height: 164rpx;
    position: relative;
}
.title{
    font-size: 30rpx;

}
.time{
    position: absolute;
    bottom: 0;
    left: 0;
    color: #d0d0d0;
    font-size: 24rpx;
} 